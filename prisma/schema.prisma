// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model Exchange {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  contracts ContractsTraded[]
  openInterest OpenInterest[]
  volumeNotional VolumeNotional[]
  volumePremium VolumePremium[]
}
model CoinCurrency {
  id        Int      @id @default(autoincrement())
  name      String   @unique
  contractsTraded ContractsTraded[]
  openInterest OpenInterest[]
  volumeNotional VolumeNotional[]
  volumePremium VolumePremium[]
}


model TimeInterval {
    id    Int    @id  @default(autoincrement())
    name  String
    contractsTraded ContractsTraded[]
    openInterest OpenInterest[]
    volumeNotional VolumeNotional[]
    volumePremium VolumePremium[]
  }

model ContractsTraded {
  id Int @id @default(autoincrement())
  coinCurrency CoinCurrency  @relation(fields: [coinCurrencyID], references: [id])
  exchange Exchange @relation(fields: [exchangeID], references: [id])
  coinCurrencyID Int
  exchangeID Int
  timestamp  Int
  timeInterval TimeInterval  @relation(fields: [timeIntervalId], references: [id])
  timeIntervalId Int
  value      Decimal

  @@index([coinCurrencyID])
  @@index([exchangeID])
  @@index([timeIntervalId])
}

model OpenInterest {
  id Int @id @default(autoincrement())
  coinCurrency CoinCurrency  @relation(fields: [coinCurrencyID], references: [id])
  exchange Exchange @relation(fields: [exchangeID], references: [id])
  coinCurrencyID Int
  exchangeID Int
  timestamp  Int
  timeInterval TimeInterval  @relation(fields: [timeIntervalId], references: [id])
  timeIntervalId Int
  value      Decimal

  @@index([coinCurrencyID])
  @@index([exchangeID])
  @@index([timeIntervalId])
}

model VolumeNotional {
  id Int @id @default(autoincrement())
  coinCurrency CoinCurrency  @relation(fields: [coinCurrencyID], references: [id])
  exchange Exchange @relation(fields: [exchangeID], references: [id])
  coinCurrencyID Int
  exchangeID Int
  timestamp  Int
  timeInterval TimeInterval  @relation(fields: [timeIntervalId], references: [id])
  timeIntervalId Int
  value      Decimal

  @@index([coinCurrencyID])
  @@index([exchangeID])
  @@index([timeIntervalId])
}

model VolumePremium {
  id Int @id @default(autoincrement())
  coinCurrency CoinCurrency  @relation(fields: [coinCurrencyID], references: [id])
  exchange Exchange @relation(fields: [exchangeID], references: [id])
  coinCurrencyID Int
  exchangeID Int
  timestamp  Int
  timeInterval TimeInterval  @relation(fields: [timeIntervalId], references: [id])
  timeIntervalId Int
  value      Float

  @@index([coinCurrencyID])
  @@index([exchangeID])
  @@index([timeIntervalId])
}


model ByBitData {
  id  Int  @id  @default(autoincrement())
  name String
  timeStamp DateTime
  price Float
  volume  Float
  instrument String
}

model OKEXData {
  id  Int  @id  @default(autoincrement())
  name String
  timeStamp DateTime
  price Float
  volume  Float
  instrument String
  instFamily String
}

model bitCom {
  id  Int  @id  @default(autoincrement())
  name String
  timeStamp DateTime
  price Float
  volume  Float
  instrument String
}

model binanceData {
  id  Int  @id  @default(autoincrement())
  name String
  timeStamp DateTime
  price Float
  volume  Float
  instId String
  instFamily String
}

model DeribitData {
  id  Int  @id  @default(autoincrement())
  name String
  timeStamp DateTime
  price Float
  volume  Float
  instrument String
  instFamily String
  iv Float
  trade_id String
  markPrice Float
}

